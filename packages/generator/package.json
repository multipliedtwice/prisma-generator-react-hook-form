{
  "name": "prisma-generator-react-hook-form",
  "description": "Prisma generator of React Hook Form",
  "version": "1.0.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-react-hook-form": "dist/bin.js"
  },
  "engines": {
    "node": ">=20.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest",
    "coverage": "jest --coverage",
    "prepublishOnly": "node copy.js "
  },
  "dependencies": {
    "@prisma/client": "5.14.0",
    "@prisma/generator-helper": "5.14.0",
    "@prisma/sdk": "4.0.0",
    "ejs": "^3.1.10",
    "lodash": "^4.17.21",
    "prettier": "3.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@prisma/internals": "^5.14.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/node": "20.12.13",
    "@types/prettier": "3.0.0",
    "gulp": "^5.0.0",
    "gulp-copy": "^4.0.1",
    "jest": "29.7.0",
    "prisma": "5.15.0",
    "semantic-release": "^23.1.1",
    "ts-jest": "29.1.4",
    "typescript": "5.4.5"
  },
  "homepage": "https://github.com/multipliedtwice/prisma-generator-react-hook-form/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/multipliedtwice/prisma-generator-react-hook-form.git"
  },
  "author": "multipliedtwice",
  "keywords": [
    "prisma",
    "react-hook-form",
    "frontend",
    "generator"
  ],
  "files": [
    "dist/**/*",
    "src/**/*",
    "../../README.md",
    "../../LICENSE"
  ],
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
